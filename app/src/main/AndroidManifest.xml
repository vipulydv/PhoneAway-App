<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.pclubproject.phoneaway">

    <uses-permission android:name="android.permission.RECEIVE_SMS" />
    <uses-permission android:name="android.permission.READ_SMS" />
    <uses-permission android:name="android.permission.SEND_SMS" />
    <uses-permission android:name="android.permission.WRITE_SETTINGS" />
    <uses-permission android:name="android.permission.CHANGE_CONFIGURATION" />
    <uses-permission android:name="android.permission.MODIFY_AUDIO_SETTINGS" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />
    <uses-permission android:name="android.permission.WAKE_LOCK" />
    <uses-permission android:name="android.permission.READ_CALL_LOG" />
    <uses-permission android:name="android.permission.READ_CONTACTS" />
    <uses-permission android:name="android.permission.VIBRATE" />
    <uses-permission android:name="android.permission.READ_EXTERNAL_STORAGE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_logo"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".Splash"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.MainActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".SignupActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.SignupActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".LoginActivity"
                  android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.LoginActivity" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".profile"
            android:label="Profile Setup"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.profile" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Ring"
            android:label="Ring Setup"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Ring" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Login"
            android:label="Login"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Login" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Imei"
            android:label="IMEI Number"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Imei" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Passcode"
            android:label="Passcode"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Passcode" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ChangePasscode"
            android:label="Passcode"
            android:parentActivityName=".Passcode"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".Passcode" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.ChangePasscode" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity android:name=".LockSetup"
                  android:screenOrientation="portrait"></activity>
        <activity
            android:name=".AttentionWords"
            android:label="Attention Words"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.AttentionWords" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Help"
            android:label="Help"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Help" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ForgotPassword"
            android:label="ForgotPassword"
            android:parentActivityName=".LoginActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".LoginActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.ForgotPassword" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Contacts"
            android:label="Fetch Contacts"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Contacts" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".Message_log"
            android:label="History"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.Message_log" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <activity
            android:name=".About"
            android:label="About"
            android:parentActivityName=".MainActivity"
            android:screenOrientation="portrait">
            <meta-data
                android:name="android.support.Parent_Activity"
                android:value=".MainActivity" />

            <intent-filter>
                <action android:name="com.pclubproject.phoneaway.About" />

                <category android:name="android.intent.category.DEFAULT" />
            </intent-filter>
        </activity>

        <receiver android:name=".SmsReceiver">
            <intent-filter android:priority="999">
                <action android:name="android.provider.Telephony.SMS_RECEIVED" />
                <action android:name="android.media.RINGER_MODE_CHANGED" />
            </intent-filter>
        </receiver>
        <receiver android:name=".SimStateReceiver">
            <intent-filter>
                <action android:name="android.intent.action.SIM_STATE_CHANGED" />
            </intent-filter>
        </receiver>

    </application>

</manifest>